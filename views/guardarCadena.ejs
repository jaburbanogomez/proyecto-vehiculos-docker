   <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
   <script type="module" src="firebaseconect.js"></script>
   <script type="module" src="contact.js"></script>

</head>

<body>
   <!-- header section start -->
   <div class="header_section">
      <div class="container">
         <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="/index"><img src="/images/logo.png"></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
               aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
               <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
               <ul class="navbar-nav ml-auto">

                  <li class="nav-item">
                     <a class="nav-link" href="/registrar">Nuevo Bloque</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link" href="actualizar">Actualizar Bloque</a>
                  </li>
                  <li class="nav-item">
                     <a class="nav-link" href="/index">Salir <span class="fa fa-arrow-right"></span></a>
                     
                        
                    
                  </li>
               </ul>
               <form class="form-inline my-2 my-lg-0">
               </form>
            </div>
         </nav>
      </div>
   </div>
   <!-- header section end -->
   <div class="call_text_main">
      <div class="container">
         <div class="call_taital">
            <div class="call_text"><a href="#"><i class="fa fa-map-marker" aria-hidden="true"></i><span
                     class="padding_left_15">Ubicación</span></a></div>
            <div class="call_text"><a href="#"><i class="fa fa-phone" aria-hidden="true"></i><span
                     class="padding_left_15">(+57) 3128294529</span></a></div>
            <div class="call_text"><a href="#"><i class="fa fa-envelope" aria-hidden="true"></i><span
                     class="padding_left_15">georgecars2023@gmail.com</span></a></div>
         </div>
      </div>
   </div>
   <!-- contact section start -->

   <div class="gallery_section layout_padding2">
      <div class="container">
         <div class="row">
            <div class="col-md-12">
               <h1 class="gallery_taital">Trazabilidad del Vehiculo</h1>
            </div>
         </div>
         <div class="contact_section layout_padding">
            <div class="row">
               <div class="contact_section layout_padding">
                  <div class="container">
                     <div class="row">
                        <div class="col-sm-12">
                           <h1 class="contact_taital">Ponte en Contacto</h1>
                        </div>
                     </div>
                  </div>
                  <div class="col-md-4">
                     <div class="gallery_box">
                        <div class="gallery_img"><img src="images/img-2.png"></div>
                     </div>
                  </div>
                  <form id="formulario-cadenas" action="/registrar" method="POST">
                     <div class="container">
                        <div class="contact_section_3">
                           <div class="row">
                              <div class="col-md-12">
                                 <div class="col-sm-12">
                                    <h1 class="contact_taital">Ingrese Datos del Vehiculos</h1>
                                 </div>
                                 <div class="mail_section_1">
                                    <button class="mail_button" name="action" value="consultarListado">
                                       <span class="fa fa-arrow-right"></span>
                                    </button>
                                    <input type="text" class="mail_text" placeholder="ID" id="placa" name="placa"
                                       autocomplete="off" value="<%= vehiculo.placa %>" />
                                    <input type="date" class="mail_text" placeholder="date" id="date" name="fecha"
                                       autocomplete="off" value="<%= vehiculo.fecha%>" />
                                    <input type="text" class="mail_text" placeholder="Marca" id="marca" name="marca"
                                       autocomplete="off" value="<%= vehiculo.marca%>" />
                                    <input type="text" class="mail_text" placeholder="Linea" id="linea" name="linea"
                                       autocomplete="off" value="<%= vehiculo.linea%>" />
                                    <input type="text" class="mail_text" placeholder="Numero Motor" id="numeroMotor"
                                       name="numeroMotor" autocomplete="off" value="<%= vehiculo.numeroMotor%>" />
                                    <input type="text" class="mail_text" placeholder="Numero Chasis" id="numeroChasis"
                                       name="numeroChasis" autocomplete="off" value="<%= vehiculo.numeroChasis%>" />
                                    <input type="text" class="mail_text" placeholder="Propietario" id="propietario"
                                       name="propietario" autocomplete="off" value="<%= vehiculo.propietario%>" />
                                    <input type="text" class="mail_text" placeholder="Identificacion" id="iden_pro"
                                       name="identificacion" autocomplete="off" value="<%= vehiculo.identificacion%>" />
                                    <div class="send_bt">
                                       <button class="mail_button" value="registrar" name="action" id="submit"
                                          type="submit">Encriptar</button>
                                    </div>
                                 </div>
                              </div>
                           </div>
                        </div>
                     </div>
                  </form>
               </div>
            </div>
         </div>
      </div>
   </div>
   </form>
   <script type="text/javascript" src="contact.js"></script>
   <!-- contact section end -->
   <!-- footer section start -->
   <!-- footer section end -->
   <!-- copyright section start -->
